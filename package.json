            },
            {
                "title": "to varTypes or to VAR_TYPES",
                "category": "varTypes",
                "command": "varTypes.to varTypes or to VAR_TYPES"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "eslint": "^4.6.1",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "underscore.string": "^3.3.4"
    }
