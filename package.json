{
    "name": "vartypes",
    "displayName": "varTypes",
    "description": "Format strings to camelize, dasherize, etc.",
    "version": "0.0.1",
    "publisher": "zimplexing",
    "engines": {
        "vscode": "^1.15.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:varTypes.to var-types or to varTypes",
        "onCommand:varTypes.to varTypes or to VAR_TYPES"
